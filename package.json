  "author": "LabLoop Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@fastify/compress": "^8.1.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/mongodb": "^9.0.2",
    "@fastify/multipart": "^9.2.1",
    "@fastify/rate-limit": "^10.3.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "jiti": "^2.5.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.0",
    "pino": "^9.9.4",
    "pino-pretty": "^13.1.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "globals": "^16.3.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
