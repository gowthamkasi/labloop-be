{
  "name": "labloop-backend",
  "version": "1.0.0",
  "description": "LabLoop Healthcare Lab Management System - High-performance Fastify backend with clean architecture",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts --fix",
    "seed:superadmin": "tsx src/scripts/createSuperAdmin.ts"
  },
  "keywords": [
    "fastify",
    "healthcare",
    "lab-management",
    "mongodb",
    "typescript",
    "clean-architecture",
    "hipaa-compliant"
